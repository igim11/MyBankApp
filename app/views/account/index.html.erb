<div class="card">
  <div class="card-body">
    <h6>My Savings Account</h6>
    <h1><%= @available_balance %></h1>
  </div>
</div>
<div class="account_btns">
    <%= link_to "Deposit", transactions_deposit_path, class:"btn btn-primary" %>
    <%= link_to "Transfer", transactions_transfer_path, class:"btn btn-primary" %>
    <%= link_to "Withdraw", transactions_withdraw_path, class:"btn btn-primary" %>
</div>
<div>
    <%= button_to "Sign Out", destroy_user_session_path, method: :delete, class:"btn btn-danger" %>
</div